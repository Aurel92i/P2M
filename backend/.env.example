# Backend environment configuration
# Copy this file to .env and fill with real values

# Server Configuration
PORT=4000
NODE_ENV=development

# Database Configuration
# PostgreSQL connection string
DATABASE_URL=postgresql://user:password@localhost:5432/p2m

# JWT Authentication
# Generate a strong secret with: openssl rand -base64 32
JWT_SECRET=change-me-to-a-strong-random-secret
JWT_EXPIRES_IN=7d

# Google APIs
GOOGLE_MAPS_KEY=your-google-maps-api-key
GOOGLE_VISION_KEY=your-google-vision-api-key

# Email Configuration
EMAIL_FROM=no-reply@example.com
SMTP_URL=smtp://user:pass@mail.example.com
